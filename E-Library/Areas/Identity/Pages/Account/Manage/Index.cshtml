@page
@model IndexModel
@{
    ViewData["Title"] = "Profile";
    ViewData["ActivePage"] = ManageNavPages.Index;
}

<h3>@ViewData["Title"]</h3>
<partial name="_StatusMessage" for="StatusMessage" />
<div class="row">
    <div class="col-md-6">
        <div class="list-group">
            <a asp-area="Identity" asp-page="/Account/Manage/ChangePassword" class="list-group-item list-group-item-action">Change Password</a>
            <a asp-area="Identity" asp-page="/Account/Manage/PersonalData" class="list-group-item list-group-item-action">Personal Data</a>
            <a asp-area="Identity" asp-page="/Account/Manage/DeletePersonalData" class="list-group-item list-group-item-action">Delete Personal Data</a>
        </div>
    </div>
</div>

@section Scripts {
<partial name="_ValidationScriptsPartial" />
}
